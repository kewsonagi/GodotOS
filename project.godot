; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="GodotOS"
config/description="A fake operating system interface, created by popcar2 for fun!"
config/version="1.1.0"
run/main_scene="res://Scenes/Main/MainScene.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="GodotOS"
config/features=PackedStringArray("4.5", "GL Compatibility")
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/image="uid://bcaphivalhf06"
config/icon="res://icon.svg"

[autoload]

IndieBlueprintSaveManager="*res://addons/ninetailsrabbit.indie_blueprint_save/src/save_manager.gd"
GuiTransitions="*res://addons/simple-gui-transitions/singleton.gd"
InputHelper="*res://addons/input_helper/input_helper.gd"
DefaultValues="*res://Scenes/Autoloads/Default Values/default_values.tscn"
GlobalValues="*res://Scenes/Autoloads/GlobalValues/global_values.tscn"
UVPosition="*res://Games/Paint Splatter 3D/scripts/uv_position.gd"
HotkeyManager="res://Scenes/Autoloads/Hotkey Manager/hotkey_manager.tscn"
ContextMenu="*res://Scenes/Autoloads/Context Menu/context_menu.tscn"
NotificationManager="*res://Scenes/Autoloads/Notification Manager/notification_manager.tscn"
CopyPasteManager="*res://Scenes/Autoloads/CopyPaste Manager/copy_paste_manager.tscn"
TransitionManager="*res://Games/UnderworldPlatformer/GlobalScripts/UnderworldTransition_Manager.gd"
UnderworldGlobal="*res://Games/UnderworldPlatformer/GlobalScripts/UnderworldGlobal.gd"
UnderworldQuest="*res://Games/UnderworldPlatformer/GlobalScripts/UnderworldQuestManager.gd"

[debug]

gdscript/warnings/untyped_declaration=1

[display]

window/size/mode=2
window/size/initial_position_type=3
window/size/transparent=true
window/energy_saving/keep_screen_on=false
window/subwindows/embed_subwindows=false
window/vsync/vsync_mode=2

[dotnet]

project/assembly_name="GodotOS"

[editor_plugins]

enabled=PackedStringArray("res://addons/ColorPreview/plugin.cfg", "res://addons/InputMapperPresetLoader/plugin.cfg", "res://addons/Previewer_2D_3D/plugin.cfg", "res://addons/TweenAnimator/plugin.cfg", "res://addons/audio_manager/plugin.cfg", "res://addons/editor_log_highlighter/plugin.cfg", "res://addons/external_editor/plugin.cfg", "res://addons/file_previewer/plugin.cfg", "res://addons/hein_draw/plugin.cfg", "res://addons/input_helper/plugin.cfg", "res://addons/ninetailsrabbit.indie_blueprint_save/plugin.cfg", "res://addons/script-ide/plugin.cfg", "res://addons/simple-gui-transitions/plugin.cfg")

[file_customization]

folder_colors={
"res://Art/": "green",
"res://Default Files/": "gray",
"res://Games/": "red",
"res://Scenes/": "blue"
}

[filesystem]

import/fbx/enabled=true

[global_group]

color+contrast_shader=""
Danger=""
Level=""
Music_Player=""
Player=""
vignette_shader=""

[gui]

theme/default_font_hinting=2
theme/default_font_multichannel_signed_distance_field=true
theme/lcd_subpixel_layout=0

[gui_transitions]

config/default/auto_start=true
config/default/fade_layout=true
config/default/center_pivot=true
config/default/duration=0.5
config/default/delay=0.5
config/default/transition_type="QUAD"
config/default/ease_type="IN_OUT"
config/default/animation_enter=4
config/default/animation_leave=4

